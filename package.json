{
    "name": "bot-boi",
    "version": "1.0.0",
    "description": "Discord Utility Bot",
    "main": "index.js",
    "repository": "git@github.com:bdatdo0601/Discord-Bot-Boi.git",
    "author": "Dat Bac Do <dat.b.do@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "start": "rm -rf ./node_modules/.cache/babel-loader && nodemon --exec babel-node -- src/index.js",
        "build-server": "babel -d ./build ./src -s --copy-files",
        "clean": "rm -rf build && mkdir build",
        "build": "yarn clean && yarn build-server",
        "start-production": "NODE_ENV=production node ./build/index.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "discord.js": "^11.4.2",
        "dotenv": "^6.0.0",
        "ffmpeg-binaries": "^4.0.0",
        "lodash": "^4.17.10",
        "node-opus": "^0.3.0",
        "xml-js": "^1.6.7",
        "xml2json": "^0.11.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.0.20",
        "babel-eslint": "^8.2.3",
        "babel-plugin-import-graphql": "^2.5.2",
        "babel-plugin-inline-import": "^3.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.0.16",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "nodemon": "^1.17.3",
        "prettier": "^1.12.1"
    }
}
